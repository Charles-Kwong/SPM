/* pages/mgr-index/client_management/employee_list/employee_list.wxss */
.container{
  height: 1500rpx;
}
.container .search{
  height: 65rpx;
  padding: 12.5rpx 0 12.5rpx 15rpx;
  background: #377EE8;
}
.container .search-left{
  flex: 8;
  text-align: left;
}
.container .search-left input{
  display: inline-block;
  width: 550rpx;
  height: 65rpx;
  padding-left: 20rpx;
  color: #666;
  font-size: 26rpx;
  background: #fff;
}
.search-placeholder{
  color: #aaa;
  line-height: 20rpx;
}
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}
.container .search .search-right{
  flex: 1;
}
.container .search .search-right image{
  width: 45rpx;
  height: 45rpx;
  padding: 10rpx;
}
.space-line{
    display: block;
    height: 2rpx;
    width: 100%;
}
.container{
  padding: 0;
  font-size: 14rpx;
  /* background: #F0F4F3; */
  color: #000;
}